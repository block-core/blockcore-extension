{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "blockcore-extension": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "angular",
      "sourceRoot": "angular/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/extension",
            "index": "angular/src/index.html",
            "main": "angular/src/main.ts",
            "polyfills": "angular/src/polyfills.ts",
            "tsConfig": "angular/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "styles": [
              "angular/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "buildOptimizer": false,
              "optimization": {},
              "vendorChunk": false,
              "extractLicenses": true,
              "sourceMap": false,
              "namedChunks": true,
              "outputHashing": "all",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "6mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "assets": [
                "angular/src/net_request_rules.json",
                "angular/src/provider.js",
                "angular/src/content.js",
                {
                  "input": "angular/src/assets/blockcore/",
                  "output": "/assets",
                  "glob": "*"
                },
                {
                  "input": "angular/src/assets/i18n/",
                  "output": "/assets/i18n",
                  "glob": "*"
                },
                {
                  "input": "angular/src/manifest/blockcore/",
                  "output": "/",
                  "glob": "manifest.json"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "angular/src/environments/environment.ts",
                  "with": "angular/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": true,
              "sourceMap": true,
              "namedChunks": true,
              "assets": [
                "angular/src/net_request_rules.json",
                "angular/src/provider.js",
                "angular/src/content.js",
                {
                  "input": "angular/src/assets/blockcore/",
                  "output": "/assets",
                  "glob": "*"
                },
                {
                  "input": "angular/src/assets/i18n/",
                  "output": "/assets/i18n",
                  "glob": "*"
                },
                {
                  "input": "angular/src/manifest/blockcore/",
                  "output": "/",
                  "glob": "manifest.json"
                }
              ]
            },
            "coinvault": {
              "buildOptimizer": false,
              "optimization": {},
              "vendorChunk": false,
              "extractLicenses": true,
              "sourceMap": false,
              "namedChunks": true,
              "outputHashing": "all",
              "outputPath": "dist/extension",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "angular/src/environments/environment.ts",
                  "with": "angular/src/environments/environment.coinvault.ts"
                }
              ],
              "assets": [
                "angular/src/net_request_rules.json",
                "angular/src/provider.js",
                "angular/src/content.js",
                {
                  "input": "angular/src/assets/coinvault/",
                  "output": "/assets",
                  "glob": "*"
                },
                {
                  "input": "angular/src/assets/i18n/",
                  "output": "/assets/i18n",
                  "glob": "*"
                },
                {
                  "input": "angular/src/manifest/coinvault/",
                  "output": "/",
                  "glob": "manifest.json"
                }
              ]
            },
            "smartcityplatform": {
              "buildOptimizer": false,
              "optimization": {},
              "vendorChunk": false,
              "extractLicenses": true,
              "sourceMap": false,
              "namedChunks": true,
              "outputHashing": "all",
              "outputPath": "dist/extension",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "assets": [
                "angular/src/net_request_rules.json",
                "angular/src/provider.js",
                "angular/src/content.js",
                {
                  "input": "angular/src/assets/smartcityplatform/",
                  "output": "/assets",
                  "glob": "*"
                },
                {
                  "input": "angular/src/assets/i18n/",
                  "output": "/assets/i18n",
                  "glob": "*"
                },
                {
                  "input": "angular/src/manifest/smartcityplatform/",
                  "output": "/",
                  "glob": "manifest.json"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "angular/src/environments/environment.ts",
                  "with": "angular/src/environments/environment.smartcityplatform.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "options": {
            "port": 4201
          },
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "blockcore-extension:build:production"
            },
            "development": {
              "browserTarget": "blockcore-extension:build:development"
            },
            "coinvault": {
              "browserTarget": "blockcore-extension:build:coinvault"
            },
            "smartcityplatform": {
              "browserTarget": "blockcore-extension:build:smartcityplatform"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "blockcore-extension:build"
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "angular/src/test.ts",
            "polyfills": "angular/src/polyfills.ts",
            "tsConfig": "angular/tsconfig.spec.json",
            "karmaConfig": "angular/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "angular/src/favicon.ico",
              "angular/src/assets"
            ],
            "styles": [
              "angular/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "blockcore-extension"
}